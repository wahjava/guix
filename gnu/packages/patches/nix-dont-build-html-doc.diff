 	fi
 
-# Generate the HTML manual.
-.PHONY: manual-html
-manual-html: $(docdir)/manual/index.html
-install: $(docdir)/manual/index.html
-
 # Generate 'nix' manpages.
 .PHONY: manpages
 manpages: $(mandir)/man1/nix3-manpages
